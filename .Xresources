!extensions
URxvt.perl-ext-common: default,matcher,tabbedex,clipboard

!clickable urls
URxvt.url-launcher: /usr/bin/google-chrome-stable
URxvt.matcher.button: 1

!change copy/paste from ctrl-alt-c,v to ctrl-shift-c,v
URxvt.iso14755: False
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.clipboard.copycmd:  xsel -ib
URxvt.clipboard.pastecmd: xsel -ob

URxvt*colorUL:                #859900
URxvt*colorBD:                #268bd2
URxvt*color1:                 #dc322f
URxvt*color9:                 #cb4b16
URxvt*color2:                 #859900
URxvt*color3:                 #b58900
URxvt*color4:                 #268bd2
URxvt*color5:                 #d33682
URxvt*color13:                #6c71c4
URxvt*color6:                 #2aa198

!URxvt*background:             #002b36
URxvt*foreground:             #839496
URxvt*fadeColor:              #002b36
URxvt*cursorColor:            #93a1a1
URxvt*pointerColorBackground: #586e75
URxvt*pointerColorForeground: #93a1a1
URxvt*scrollColor:            #657b83
URxvt*borderColor:            #002b36
URxvt*color0:                 #073642
URxvt*color8:                 #839496
URxvt*color10:                #586e75
URxvt*color11:                #657b83
URxvt*color12:                #839496
URxvt*color14:                #93a1a1
URxvt*color7:                 #eee8d5
URxvt*color15:                #fdf6e3

URxvt*cursorBlink:                    false
URxvt*internalBorder:                 0
URxvt*font:                           xft:Anonymous Pro:pixelsize=18
URxvt*scrollBar:                      off
URxvt*scrollWithBuffer:               true
URxvt*scrollTtyOutput:                false
URxvt*scrollTtyKeypress:              true
URxvt.depth: 32
!URxvt.background: [80]#002b36
!alpha channel is transparency 65535 = ffff -> take %
URxvt*background: rgba:0000/2b00/3600/f332
URxvt*clipboard.autocopy: true
URxvt*dynamicColors:                  on
URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0
